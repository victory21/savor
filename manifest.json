{
    "name" : "Savor",
    "version" : "0.0.1",
    "manifest_version" : 2,
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["https://www.ubereats.com/"],
          "js": ["content.js"]
        },
        {
          "matches": ["https://www.chipotle.com/"],
          "js": ["chipotle.js"]
        },
        {
          "matches": ["https://www.nike.com/"],
          "js": ["nike.js"]
        }
      ]
}